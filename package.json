    "test": "jest",
    "build:wp": "webpack",
    "dev-server": "webpack-dev-server"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.24.1",
    "body-parser": "^1.18.3",
    "css-loader": "0.28.4",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-session": "^1.15.6",
    "install": "^0.10.2",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.0",
    "normalize.css": "7.0.0",
    "password-hash": "^1.2.2",
    "path": "^0.12.7",
    "pg": "^7.4.3",
