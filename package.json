{
  "name": "node-js-getting-started",
  "version": "0.3.0",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "8.9.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "serve:server": "nodemon index.js",
    "push": "git push github master; git push heroku master",
    "build:sass": "node-sass ./private/scss/index.scss ./public/styles/styles.css",
    "watch": "node-sass ./private/scss/index.scss ./public/styles/styles.css -w",
